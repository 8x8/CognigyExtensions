{
  "name": "blueprism",
  "version": "1.0.0",
  "description": "Integrates with Blueprism RPA",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz blueprism.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Cognigy.AI",
    "example",
    "extensions",
    "documentation"
  ],
  "author": "Cognigy GmbH",
  "license": "ISC",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "node-fetch": "^2.6.1",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.7",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
