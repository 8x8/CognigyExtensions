    "parcel": "^1.12.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-microsoft-login": "^1.12.1"
  },
  "alias": {
    "react": "./alias/react"
  }
}
